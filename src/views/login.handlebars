<div class="login-container">
  <form id="login-form" class="login-form">
    <h2>Iniciar sesión</h2>
    <div class="form-group">

      <label for="username">Usuario:</label>
      <input type="text" id="username" name="username" required />
    </div>
    <div class="form-group">

      <label for="password">Contraseña:</label>
      <input type="password" id="password" name="password" autocomplete="on" required />

    </div>
    <button id="login" type="submit">Ingresar</button>
  </form>
    
    <div>
      <a href="/api/session/github"><button>Github</button></a>
    </div>
    
    <button id="registrate">Registrate</button>
    <button id="forgotPass">Recuperar contraseña</button>
</div>
<script src="js/login.js"></script>